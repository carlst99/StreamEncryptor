<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Carl Stephens</Authors>
    <Product />
    <Description>A small helper library to encrypt, decrypt and authenticate streams</Description>
    <PackageProjectUrl>https://github.com/carlst99/StreamEncryptor</PackageProjectUrl>
    <PackageIconUrl></PackageIconUrl>
    <RepositoryUrl>https://github.com/carlst99/StreamEncryptor</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>encryption authentication streams</PackageTags>
    <NeutralLanguage>en-NZ</NeutralLanguage>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseUrl>https://github.com/carlst99/StreamEncryptor/blob/master/LICENSE</PackageLicenseUrl>
    <Version>1.1.0</Version>
    <PackageReleaseNotes>Performance enhancements - you can now pass an output stream directly to the EncryptAsync and DecryptAsync methods of an Encryptor</PackageReleaseNotes>
  </PropertyGroup>

</Project>
